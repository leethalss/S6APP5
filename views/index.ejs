<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Archives</title>
  <style>
    table {
      width: 100%;
      border-collapse: collapse;
    }
    th, td {
      padding: 8px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }
    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
  <h1>Archives</h1>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Time</th>
        <th>Type</th>
      </tr>
    </thead>
    <tbody>
      <% archives.forEach(function(archive) { %>
        <tr>
          <td><%= archive.id %></td>
          <td><%= archive.time %></td>
          <td><%= archive.type %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
